#wrapper
	#content
		#rank
			overflow auto
			table
				border-collapse collapse
				width 100%
				font-family 'Secular One', 'Noto Sans Korean', sans-serif
				color #FFF
				tr
					border-bottom 1px solid #FFF
					&:nth-of-type(1)
						background-color #2196F3
						border 0
						td
							border 0
							text-align center
							font-weight 900
					&:not(:nth-of-type(1))
						&:hover
							background-color #757575
				td
					font-size 1.2em
					padding .5em
					border-bottom 2px solid #FFF
					&:nth-of-type(1)
						width 5%
					&:nth-of-type(2)
						width 15%
					&:nth-of-type(3)
						width 50%
					&:nth-of-type(4)
						width 10%
					&:nth-of-type(5)
						width 20%
					&:not(:nth-of-type(3))
						text-align center
					&:not(:first-child)
						border-left 1px solid #FFF
						